{
    "title": "君子居",
    "author": "风戏fw",
    "description": "记录学习笔记，以备后续查询、查漏补缺",
    "language": "zh-hans",
    "gitbook": "3.2.3",
    "links": {
	"links": {
		"sidebar": {
            "博客园": "https://www.cnblogs.com/fwpsl"
        }
    },
    "plugins": [
		"back-to-top-button",
		"chapter-fold",
		"code",
		"todo",
		"-lunr", 
        "-search", 
        "search-pro",
		"advanced-emoji",
		"emphasize",
		"splitter",
		"-sharing", 
		"sharing-plus",
		"tbfed-pagefooter",
		"sectionx",
		"alerts",
		"popup",
		"click-reveal",
		"hide-element",
		"page-treeview"		
    ],
    "pluginsConfig": {
		"sharing": {
             "douban": true,
             "facebook": false,
             "google": false,
             "pocket": false,
             "qq": true,
             "qzone": true,
             "twitter": false,
             "weibo": true
       },
	   "tbfed-pagefooter": {
            "copyright":"Copyright &copy fwpsl.top 2019",
            "modify_label": "该文件修订时间：",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
		"page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
		"hide-element": {
            "elements": [".gitbook-link"]
        },
		"page-treeview": {
            "copyright": "Copyright &#169; aleen42",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        }
    }
}